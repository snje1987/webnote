{
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/snje1987/gitter"
        }
    ],
    "require": {
        "snje1987/minifw": "master@dev",
        "snje1987/markdown": "master@dev",
        "klaussilveira/gitter": "dev-master",
        "twbs/bootstrap": "3.*",
        "components/jquery": "3.*",
        "jquery-form/form": "4.*",
        "components/highlightjs": "9.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*"
    },
    "autoload": {
        "psr-4": {
            "Org\\Snje\\Webnote\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Org\\Snje\\WebnoteTest\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "Org\\Snje\\Webnote\\Model\\Resource::copy_composer_resource"
    }
}
