{
    "require": {
        "snje1987/minifw2": "^1.0",
        "snje1987/markdown": "master@dev",
        "snje1987/gitter": "dev-master",
        "twbs/bootstrap": "3.*",
        "components/jquery": "3.*",
        "jquery-form/form": "4.*",
        "components/highlightjs": "9.*",
        "snapappointments/bootstrap-select": "1.*",
        "gkralik/php-uglifyjs": "dev-master",
        "natxet/cssmin": "dev-master",
        "fortawesome/font-awesome": "^4.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "php ./bin/cmd.php system copy_resource"
    }
}
